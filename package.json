{
    "name": "netlify-subdomain-manager",
    "version": "0.1.1",
    "description": "A CLI tool used to dynamically manage your subdomain on Netlify.",
    "author": "Haseeb Majid",
    "license": "MIT",
    "homepage": "https://gitlab.com/hmajid2301/netlify-subdomain-manager#readme",
    "main": "lib/index.js",
    "repository": {
        "type": "git",
        "url": "git@gitlab.com:hmajid2301/netlify-subdomain-manager.git"
    },
    "bugs": {
        "url": "https://gitlab.com/hmajid2301/netlify-subdomain-manager/-/issues"
    },
    "bin": {
        "netlify_subdomain_manager": "./lib/index.js"
    },
    "keywords": [
        "netlify",
        "cli",
        "script"
    ],
    "scripts": {
        "build": "babel --presets es2015 -d lib/ src",
        "start": "babel-node ./src",
        "code-formatter": "prettier src/**/*.js --write",
        "code-formatter-check": "prettier --check src/**/*.js",
        "lint": "eslint src/**/*.js"
    },
    "dependencies": {
        "babel-plugin-transform-runtime": "6.23.0",
        "netlify": "4.1.1",
        "yargs": "15.3.0"
    },
    "devDependencies": {
        "babel-cli": "6.26.0",
        "babel-preset-env": "1.7.0",
        "babel-preset-es2015": "6.24.1",
        "eslint": "6.8.0",
        "eslint-config-airbnb-base": "14.0.0",
        "eslint-config-prettier": "6.9.0",
        "eslint-plugin-import": "2.20.1",
        "eslint-plugin-prettier": "3.1.2",
        "prettier": "1.19.1"
    }
}